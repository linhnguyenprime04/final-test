);\n  }\n\n  const tasksNoun = taskList.length !== 1 ? \"tasks\" : \"task\";\n  const headingText = `${taskList.length} ${tasksNoun} remaining`;\n\n\n  return (\n    <div className=\"todoapp stack-large\">\n      <h1>2do</h1>\n      <Form addTask={addTask} />\n      <div className=\"filters btn-group stack-exception\">\n      {filterList}\n\n      </div>\n      <h2 id=\"list-heading\">{headingText}</h2>\n      <ul\n        role=\"list\"\n        className=\"todo-list stack-large stack-exception\"\n        aria-labelledby=\"list-heading\">\n        {taskList}\n      </ul>\n    </div>\n  );\n}\n\n\nexport default App;\n","C:\\Users\\84376\\Documents\\CIJS83\\bai_test\\my-react-app\\src\\reportWebVitals.js",[],[],"C:\\Users\\84376\\Documents\\CIJS83\\bai_test\\my-react-app\\src\\components\\todo.js",[],[],"C:\\Users\\84376\\Documents\\CIJS83\\bai_test\\my-react-app\\src\\components\\form.js",["36","37"],[],"import React from \"react\";\r\nimport { useState } from \"react\";\r\n\r\n\r\n\r\n  \r\n\r\nexport default function Form(props) {\r\n    \r\n    const [name, setName] = useState(\"\");\r\n\r\n    function handleChange(e) {\r\n        setName(e.target.value);\r\n      }\r\n\r\n    \r\n    function handleSubmit(e) {\r\n        e.preventDefault();\r\n        props.addTask(\"Edit new task!\");\r\n        setName(\"\");\r\n      }\r\n    \r\n    return (\r\n        <form onSubmit={handleSubmit}>\r\n            {/* <input\r\n                type=\"text\"\r\n                id=\"new-todo-input\"\r\n                className=\"input input__lg\"\r\n                name=\"text\"\r\n                autoComplete=\"off\"\r\n                value={name}\r\n                onchange={handleChange}\r\n            /> */}\r\n            <button type=\"submit\" className=\"btn btn__primary btn__lg\">\r\n                Add\r\n            </button>\r\n        </form>\r\n    );\r\n\r\n}\r\n","C:\\Users\\84376\\Documents\\CIJS83\\bai_test\\my-react-app\\src\\components\\filterbtn.js",[],